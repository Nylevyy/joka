  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "format": "run-s format:*",
    "format:eslint": "eslint --ext .js,.jsx src",
    "format:prettier": "prettier --write \"src/**/*.{js,jsx}\"",
